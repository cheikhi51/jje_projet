<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>signup</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>UtilisateurServlet</display-name>
    <servlet-name>UtilisateurServlet</servlet-name>
    <servlet-class>web.UtilisateurServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UtilisateurServlet</servlet-name>
    <url-pattern>*.utilisateur</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProduitServlet</display-name>
    <servlet-name>ProduitServlet</servlet-name>
    <servlet-class>web.ProduitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProduitServlet</servlet-name>
    <url-pattern>*.produit</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CommandeServlet</display-name>
    <servlet-name>CommandeServlet</servlet-name>
    <servlet-class>web.CommandeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CommandeServlet</servlet-name>
    <url-pattern>*.commande</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LivraisonServlet</display-name>
    <servlet-name>LivraisonServlet</servlet-name>
    <servlet-class>web.LivraisonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LivraisonServlet</servlet-name>
    <url-pattern>/LivraisonServlet</url-pattern>
  </servlet-mapping>
</web-app>